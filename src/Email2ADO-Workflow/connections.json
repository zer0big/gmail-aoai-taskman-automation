{
  "managedApiConnections": {
    "gmail": {
      "api": {
        "id": "/subscriptions/@appsetting('AZURE_SUBSCRIPTION_ID')/providers/Microsoft.Web/locations/@appsetting('AZURE_LOCATION')/managedApis/gmail"
      },
      "connection": {
        "id": "/subscriptions/@appsetting('AZURE_SUBSCRIPTION_ID')/resourceGroups/@appsetting('AZURE_RESOURCE_GROUP')/providers/Microsoft.Web/connections/@appsetting('GMAIL_CONNECTION_NAME')"
      },
      "authentication": {
        "type": "Raw",
        "scheme": "Key",
        "parameter": "@appsetting('gmail_connectionKey')"
      }
    },
    "teams": {
      "api": {
        "id": "/subscriptions/@appsetting('AZURE_SUBSCRIPTION_ID')/providers/Microsoft.Web/locations/@appsetting('AZURE_LOCATION')/managedApis/teams"
      },
      "connection": {
        "id": "/subscriptions/@appsetting('AZURE_SUBSCRIPTION_ID')/resourceGroups/@appsetting('AZURE_RESOURCE_GROUP')/providers/Microsoft.Web/connections/@appsetting('TEAMS_CONNECTION_NAME')"
      },
      "authentication": {
        "type": "Raw",
        "scheme": "Key",
        "parameter": "@appsetting('teams_connectionKey')"
      }
    },
    "visualstudioteamservices": {
      "api": {
        "id": "/subscriptions/@appsetting('AZURE_SUBSCRIPTION_ID')/providers/Microsoft.Web/locations/@appsetting('AZURE_LOCATION')/managedApis/visualstudioteamservices"
      },
      "connection": {
        "id": "/subscriptions/@appsetting('AZURE_SUBSCRIPTION_ID')/resourceGroups/@appsetting('AZURE_RESOURCE_GROUP')/providers/Microsoft.Web/connections/@appsetting('ADO_CONNECTION_NAME')"
      },
      "authentication": {
        "type": "Raw",
        "scheme": "Key",
        "parameter": "@appsetting('vsts_connectionKey')"
      }
    }
  },
  "serviceProviderConnections": {
    "azureOpenAI": {
      "parameterValues": {
        "openAIEndpoint": "@appsetting('AZURE_OPENAI_ENDPOINT')",
        "apiVersion": "@appsetting('AZURE_OPENAI_API_VERSION')"
      },
      "serviceProvider": {
        "id": "/serviceProviders/openai"
      },
      "displayName": "Azure OpenAI (GPT-4o)",
      "authentication": {
        "type": "ManagedServiceIdentity",
        "managedIdentityId": ""
      }
    }
  }
}
